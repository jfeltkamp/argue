{
  "name": "jfeltkamp/argue",
  "description": "Argue distribution.",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Joachim Feltkamp",
      "email": "j.feltkamp@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": "^7.3",
    "cweagans/composer-patches": "^1.7",
    "league/commonmark": "^0.11",

    "drupal/core": "^9.3",
    "drupal/address": "^1.9",
    "drupal/admin_toolbar": "3.0.2",
    "drupal/antibot": "^1.4",
    "drupal/change_requests": "^2.0",
    "drupal/config_selector": "^2.0",
    "drupal/config_update": "^1.7",
    "drupal/console": "^1.9",
    "drupal/default_content": "^2.0",
    "drupal/diff": "^1.0",
    "drupal/draggable_dashboard": "2.0.0-alpha3",
    "drupal/entity": "^1.0",
    "drupal/entity_form_mode": "^2.0",
    "drupal/entity_reference_revisions": "^1.8",
    "drupal/field_group": "^3.1",
    "drupal/gin": "3.0.0-alpha36",
    "drupal/gin_toolbar": "1.0.0-beta17",
    "drupal/length_indicator": "^1.1",
    "drupal/pathauto": "^1.8",
    "drupal/r4032login": "^2.1",
    "drupal/rate": "1.0.0-alpha6",
    "drupal/login_emailusername": "^1.2",
    "drupal/menu_link_attributes": "^1.1",
    "drupal/publishcontent": "^1.2",
    "drupal/select2": "^1.13",
    "drupal/taxonomy_menu": "^3.4",
    "drupal/tour_ui": "~1.0",
    "drupal/twig_tweak": "^2.9",
    "drupal/token": "^1.0",
    "drupal/update_helper": "^2.0",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/votingapi": "^3.0",
    "drupal/votingapi_widgets": "dev-1.x",
    "npm-asset/select2": "^4.0.7",
    "npm-asset/autosize": "^5.0.0"
  },
  "require-dev": {
    "drupal/drupal-extension": "^3.1"
  },
  "config": {
    "preferred-install": "dist",
    "bin-dir": "bin/",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "argue",
    "patches": {
      "drupal/rate": {
        "Display own voting and update D9 compatibility": "patches/drupal_rate_you_voted.patch"
      },
      "drupal/draggable_dashboard": {
        "Block display issue #3217642": "patches/draggable_dashboard_block_structure.patch"
      }
    },
    "installer-paths": {
      "config/shared": [
        "type:drupal-module-config"
      ],
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-module-custom"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-profile-custom"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-theme-custom"
      ],
      "docroot/drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/drush/custom/{$name}": [
        "type:drupal-drush-custom"
      ],
      "docroot/modules/contrib/libraries/{$name}": [
        "type:drupal-library"
      ]
    }
  }
}
