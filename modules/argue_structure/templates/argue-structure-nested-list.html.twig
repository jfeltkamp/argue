

{% if level >= 0 %}
<li{{ attributes.addClass('list-item') }}>
  <strong><span>{{ label }}</span>{% if link %}&nbsp;<a
      href="{{ link }}" class="argue-icon">navigate_next</a>{% endif %}
    {% if description %}
      <div class="description">
        {{ description }}
      </div>
    {% endif %}
  </strong>
  {{ node_list }}
  <ol>
{% else %}
  <ol{{ attributes }}>
{% endif %}
    {{ items }}
  </ol>
{% if level >= 0 %}
</li>
{% endif %}
